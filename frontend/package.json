{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prietter": "prettier --write src/app",
    "prepare": "husky",
    "st": "jasmine",
    "test": "BABEL_CONFIG_FILE=babel.test.config.js jasmine --config=spec/support/jasmine.json"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.8.2",
    "axios": "^1.10.0",
    "next": "15.4.2",
    "react": "^19.1.1",
    "react-bootstrap": "^2.10.10",
    "react-bootstrap-icons": "^1.11.6",
    "react-dom": "^19.1.1",
    "react-icons": "^5.5.0",
    "react-redux": "^9.2.0",
    "sass": "^1.90.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@babel/register": "^7.27.1",
    "@eslint/eslintrc": "^3",
    "@testing-library/jasmine-dom": "^1.3.3",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@types/estree": "^1.0.8",
    "@types/identity-obj-proxy": "^3.0.2",
    "@types/jasmine": "^4.6.5",
    "@types/jsdom": "^21.1.7",
    "@types/json-schema": "^7.0.15",
    "@types/node": "^20.19.9",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/systemjs": "^6.15.3",
    "@types/testing-library__jasmine-dom": "^1.3.4",
    "bootstrap": "^5.3.7",
    "eslint": "^9.31.0",
    "eslint-config-next": "15.4.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "ignore-styles": "^5.0.1",
    "jasmine": "^3.99.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.4.0",
    "jasmine-ts-console-reporter": "^3.1.1",
    "jsdom": "^26.1.0",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "react-test-renderer": "^19.1.1",
    "systemjs": "^6.15.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all",
    "endOfLine": "lf"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "eslintConfig": {
    "root": true,
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "project": "./tsconfig.json",
      "tsconfigRootDir": ".",
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:@typescript-eslint/recommended-requiring-type-checking",
      "next/core-web-vitals"
    ]
  },
  "overrides": {
    "whatwg-url": "8.1.0"
  }
}